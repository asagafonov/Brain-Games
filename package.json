   "docsify-cli": "^4.3.0",
   "husky": "^3.0.5",
   "jest": "^24.9.0",
   "prettier": "^1.18.2",
   "ts-jest": "^24.0.2",
   "tslint": "^5.20.0",
   "tslint-config-prettier": "^1.18.0",
   "typedoc": "^0.15.0",
   "typescript": "^3.6.3"
 },
  "bin": {
    "brain-games": "src/bin/brain-games.js"
