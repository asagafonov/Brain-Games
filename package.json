  "description": "A brain games package",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "console.log(\"Welcome to the Brain Games!\")",
    "build": "tsc",
    "dev": "tsc --watch",
    "prettier": "prettier --write src/* test/* example/*",
    "tslint": "tslint -p tslint -p tsconfig.json",
    "codestyle": "npm run prettier && npm run tslint",
